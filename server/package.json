    "cors": "^2.8.5",
    "express": "^4.21.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241112.0",
    "vitest": "^1.6.0"
